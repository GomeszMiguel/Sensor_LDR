\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 115            \{\par
116     {\cf20 // Inicializa o sensor LDR com o caminho do arquivo ADC no sysfs da placa}\par
117     SensorLDR ldr({\cf22 "/sys/bus/iio/devices/iio:device0/in_voltage13_raw"});\par
118 \par
119     {\cf18 int} client_socket;\par
120     {\cf17 struct }sockaddr_in server_addr;\par
121     {\cf18 char} buffer[BUFFER_SIZE];\par
122     \par
123     {\cf20 // 1. Criar o Socket}\par
124     {\cf20 // AF_INET: Endere\'E7os IPv4}\par
125     {\cf20 // SOCK_DGRAM: Protocolo UDP (datagrama, sem conex\'E3o)}\par
126     client_socket = socket(AF_INET, SOCK_DGRAM, 0);\par
127     {\cf19 if} (client_socket < 0) \{\par
128         perror({\cf22 "Erro ao criar o socket UDP do cliente"});\par
129         {\cf19 return} -1;\par
130     \}\par
131 \par
132     {\cf20 // Limpar e configurar o endere\'E7o do servidor}\par
133     memset(&server_addr, 0, {\cf17 sizeof}(server_addr));\par
134     server_addr.sin_family = AF_INET;\par
135     server_addr.sin_port = htons(PORT); {\cf20 // Converte a porta (8080) para a ordem de bytes de rede}\par
136 \par
137     {\cf20 // Converter endere\'E7o IP de string para formato bin\'E1rio}\par
138     {\cf19 if} (inet_pton(AF_INET, SERVER_IP, &server_addr.sin_addr) <= 0) \{\par
139         perror({\cf22 "Endereco IP invalido/nao suportado"});\par
140         close(client_socket);\par
141         {\cf19 return} -1;\par
142     \}\par
143     \par
144     cout << {\cf22 "Socket UDP criado com sucesso."} << endl;\par
145 \par
149     {\cf19 while} ({\cf17 true}) \{\par
150         memset(buffer, 0, BUFFER_SIZE);\par
151 \par
152         {\cf20 // O valor lido do sensor \'E9 um INT}\par
153         {\cf18 int} val = ldr.lerLuminosidadePercentual();\par
154 \par
155         {\cf20 // CORRE\'C7\'C3O do erro: invalid conversion from 'int' to 'const char *'}\par
156         {\cf20 // 1. Converte o valor inteiro (int) para uma string C++ (std::string)}\par
157         {\cf18 string} val_str = to_string(val);\par
158 \par
159         {\cf20 // 2. Obt\'E9m o ponteiro C-style (const char*) da string para uso na fun\'E7\'E3o sendto()}\par
160         {\cf17 const} {\cf18 char} *message = val_str.c_str(); \par
161     \par
162         {\cf20 // Calcula o tamanho da mensagem (apenas o tamanho da string, sem o terminador nulo)}\par
163         {\cf18 size_t} message_len = val_str.length();\par
164 \par
174         ssize_t bytes_sent = sendto(client_socket, message, message_len, 0, ({\cf17 const} {\cf17 struct} sockaddr *)&server_addr, {\cf17 sizeof}(server_addr));\par
175 \par
176         {\cf19 if} (bytes_sent == -1) \{\par
177             perror({\cf22 "Erro ao enviar datagrama"});\par
178         \} \par
179         {\cf19 else}\{\par
180             cout << {\cf22 "Datagrama enviado ("} << bytes_sent << {\cf22 " bytes) para "} << SERVER_IP << {\cf22 ":"} << PORT << endl;\par
181             cout << {\cf22 "Luminosidade: "} << message << {\cf22 "%"} << std::endl;\par
182             sleep(1); {\cf20 // Espera 1 segundo antes da pr\'F3xima leitura/envio}\par
183         \}\par
184     \}\par
185     {\cf20 // O loop \'E9 infinito, o c\'F3digo abaixo s\'F3 seria executado em caso de interrup\'E7\'E3o}\par
186     {\cf20 // 4. Fechar o Socket}\par
187     close(client_socket);\par
188     cout << {\cf22 "Socket fechado. Cliente UDP encerrado."} << endl;\par
189     {\cf19 return} 0;\par
190 \}\par
}
