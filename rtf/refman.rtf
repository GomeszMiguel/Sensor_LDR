136     {\cf20 // Inicializa o sensor LDR com o caminho do arquivo ADC no sysfs da placa}\par
137     SensorLDR ldr({\cf22 "/sys/bus/iio/devices/iio:device0/in_voltage13_raw"});\par
138 \par
139     {\cf18 int} client_socket;\par
140     {\cf17 struct }sockaddr_in server_addr;\par
141     {\cf18 char} buffer[BUFFER_SIZE];\par
142     \par
143     {\cf20 // 1. Criar o Socket}\par
144     {\cf20 // AF_INET: Endere\'E7os IPv4}\par
145     {\cf20 // SOCK_DGRAM: Protocolo UDP (datagrama, sem conex\'E3o)}\par
146     client_socket = socket(AF_INET, SOCK_DGRAM, 0);\par
147     {\cf19 if} (client_socket < 0) \{\par
148         perror({\cf22 "Erro ao criar o socket UDP do cliente"});\par
149         {\cf19 return} -1;\par
150     \}\par
151 \par
152     {\cf20 // Limpar e configurar o endere\'E7o do servidor}\par
153     memset(&server_addr, 0, {\cf17 sizeof}(server_addr));\par
154     server_addr.sin_family = AF_INET;\par
155     server_addr.sin_port = htons(PORT); {\cf20 // Converte a porta (8080) para a ordem de bytes de rede}\par
156 \par
157     {\cf20 // Converter endere\'E7o IP de string para formato bin\'E1rio}\par
158     {\cf19 if} (inet_pton(AF_INET, SERVER_IP, &server_addr.sin_addr) <= 0) \{\par
159         perror({\cf22 "Endereco IP invalido/nao suportado"});\par
160         close(client_socket);\par
161         {\cf19 return} -1;\par
162     \}\par
163     \par
164     cout << {\cf22 "Socket UDP criado com sucesso."} << endl;\par
165 \par
170     {\cf19 while} ({\cf17 true}) \{\par
171         memset(buffer, 0, BUFFER_SIZE);\par
172 \par
173         {\cf20 // O valor lido do sensor \'E9 um INT}\par
174         {\cf18 int} val = ldr.lerLuminosidadePercentual();\par
175 \par
176         {\cf20 // Converte o valor inteiro (int) para uma string C++ (std::string)}\par
177         {\cf18 string} val_str = to_string(val);\par
178 \par
179         {\cf20 // Obt\'E9m o ponteiro C-style (const char*) da string para uso na fun\'E7\'E3o sendto()}\par
180         {\cf17 const} {\cf18 char} *message = val_str.c_str(); \par
181     \par
182         {\cf20 // Calcula o tamanho da mensagem (apenas o tamanho da string, sem o terminador nulo)}\par
183         {\cf18 size_t} message_len = val_str.length();\par
184 \par
197         ssize_t bytes_sent = sendto(client_socket, message, message_len, 0, ({\cf17 const} {\cf17 struct} sockaddr *)&server_addr, {\cf17 sizeof}(server_addr));\par
198 \par
199         {\cf19 if} (bytes_sent == -1) \{\par
200             perror({\cf22 "Erro ao enviar datagrama"});\par
201         \} \par
202         {\cf19 else}\{\par
203             cout << {\cf22 "Datagrama enviado ("} << bytes_sent << {\cf22 " bytes) para "} << SERVER_IP << {\cf22 ":"} << PORT << endl;\par
204             cout << {\cf22 "Luminosidade: "} << message << {\cf22 "%"} << std::endl;\par
205             sleep(1); {\cf20 // Espera 1 segundo antes da pr\'F3xima leitura/envio}\par
206         \}\par
207     \}\par
208     {\cf20 // O loop \'E9 infinito, o c\'F3digo abaixo s\'F3 seria executado em caso de interrup\'E7\'E3o}\par
209     {\cf20 // 4. Fechar o Socket}\par
210     close(client_socket);\par
211     cout << {\cf22 "Socket fechado. Cliente UDP encerrado."} << endl;\par
212     {\cf19 return} 0;\par
213 \}\par
